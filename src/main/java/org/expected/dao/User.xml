<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="org.expected.dao.UserDao">

	    <select id="getRecordByName" parameterType="String" resultType="User">
	        select * from   user  where user_name=#{userName}    
	    </select>
	    <select id="test" resultType="User">
	    	select user_name ,password from   user  where user_name="1";
	    </select>
	</mapper>

